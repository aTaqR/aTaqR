FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -1 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    overset
    {
        type            overset;
    }

    "(casing|hole)"//|inlet|outlet)"
    {
        type            nutkWallFunction;
        value           $internalField;
    }
    
    "(inlet|outlet)"
    {
        type            zeroGradient;
    }
}

